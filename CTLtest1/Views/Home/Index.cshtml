@model CTLtest1.Models.FileUpload




@using CTLtest1.Models

<header>
    <div class="content-wrapper">
        <div class="float-left">
            <p class="site-title">
                <a href="~/">ASP.NET Web API File Upload</a>
            </p>
        </div>
    </div>
</header>
<div id="body">
    @using (Html.BeginForm("RedirectDfAction", "Home", FormMethod.Get, new
    {
    enctype = "multipart/form-data"
    })) {
    <form name="form1" method="post" action="api/docfile" enctype="multipart/form-data">
        <div>
            @*<label>
            Browse File
        </label>*@
            @Html.LabelFor(model => model.FilePath)
            <input name="myFile" type="file" enctype='multipart/form-data' />
        </div>
        <div>
            <a href='@Url.Action("Uploaded", "HomeController")'>
                <input type="submit" value="Upload" name="Upload" />
            </a>
        </div>
    </form>
    }
</div>






